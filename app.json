      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.judo2000.MealsToGo"
    },
    "extra": {
      "eas": {
        "projectId": "036504df-969d-42e2-a874-e5653fd58715"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/036504df-969d-42e2-a874-e5653fd58715"
    }
  },
  "plugins": [
    [
      "expo-camera",
      {
        "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
      }
    ]
  ]
}
